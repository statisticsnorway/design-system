{
  "name": "ssb-design-system",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://statisticsnorway.github.io/design-system/",
  "scripts": {
    "start": "cross-env ENV=development webpack serve --mode development --config webpack/dev.config.js --open",
    "build": "cross-env ENV=production webpack --mode production --config webpack/prod.config.js --progress",
    "predeploy": "npm run build",
    "deploy": "echo 'design.ssb.no' > ./build/CNAME && gh-pages -d build",
    "test": "cross-env ENV=test jest --coverage",
    "test:watch": "cross-env ENV=test jest --watch",
    "clean-test": "cross-env ENV=test jest --no-cache --updateSnapshot --coverage",
    "clear-tests": "jest --clear-cache -- -u",
    "lint:js": "eslint src/**/*.jsx",
    "lint:js:fix": "eslint --fix src/**/*.jsx",
    "lint:css": "stylelint '**/*.scss'"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.7",
    "@statisticsnorway/ssb-component-library": "^2.0.68",
    "bootstrap": "^4.6.1",
    "core-js": "^3.21.1",
    "gh-pages": "^3.2.3",
    "history": "^5.3.0",
    "prismjs": "^1.27.0",
    "prop-types": "^15.8.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-feather": "^2.0.9",
    "react-router-dom": "^5.3.0",
    "regenerator-runtime": "^0.13.9",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.7",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-prismjs": "^2.1.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "7.29.4",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "path": "^0.12.7",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.8.3",
    "stylelint-webpack-plugin": "^3.1.1",
    "svg-url-loader": "^7.1.1",
    "terser-webpack-plugin": "^5.3.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
