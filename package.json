{
  "name": "ssb-design-system",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://statisticsnorway.github.io/design-system/",
  "scripts": {
    "start": "cross-env ENV=development webpack-dev-server --mode development --config webpack/dev.config.js --open",
    "build": "cross-env ENV=production webpack --mode production --config webpack/prod.config.js --progress",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "cross-env ENV=test jest --coverage",
    "test:watch": "cross-env ENV=test jest --watch",
    "clean-test": "cross-env ENV=test jest --no-cache --updateSnapshot --coverage",
    "clear-tests": "jest --clear-cache -- -u",
    "lint:js": "eslint src/**/*.jsx",
    "lint:css": "stylelint '**/*.scss'"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "@statisticsnorway/ssb-component-library": "^2.0.36",
    "bootstrap": "^4.5.2",
    "core-js": "^3.6.5",
    "gh-pages": "^2.2.0",
    "history": "^4.10.1",
    "prismjs": "^1.21.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-feather": "^2.0.8",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.13.7",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-prismjs": "^1.1.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "brotli-webpack-plugin": "^1.1.0",
    "cross-env": "^6.0.2",
    "css-loader": "^3.6.0",
    "dotenv": "^8.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "7.14.3",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "node-sass": "^4.14.1",
    "path": "^0.12.7",
    "sass": "^1.26.11",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "stylelint": "^10.1.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "svg-url-loader": "^3.0.3",
    "terser-webpack-plugin": "^1.4.5",
    "url-loader": "^2.3.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
