{
  "name": "ssb-design-system",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://statisticsnorway.github.io/design-system/",
  "scripts": {
    "start": "cross-env ENV=development webpack serve --mode development --config webpack/dev.config.js --open",
    "build": "cross-env ENV=production webpack --mode production --config webpack/prod.config.js --progress",
    "predeploy": "npm run build",
    "deploy": "echo 'design.ssb.no' > ./build/CNAME && gh-pages -d build",
    "test": "cross-env ENV=test jest --coverage",
    "test:watch": "cross-env ENV=test jest --watch",
    "clean-test": "cross-env ENV=test jest --no-cache --updateSnapshot --coverage",
    "clear-tests": "jest --clear-cache -- -u",
    "lint:js": "eslint src/**/*.jsx",
    "lint:js:fix": "eslint --fix src/**/*.jsx",
    "lint:css": "stylelint '**/*.scss'"
  },
  "dependencies": {
    "@babel/runtime": "~7.28.4",
    "@statisticsnorway/ssb-component-library": "~2.5.0",
    "bootstrap": "~5.3.8",
    "core-js": "~3.45.1",
    "gh-pages": "~6.3.0",
    "history": "~5.3.0",
    "prismjs": "~1.30.0",
    "prop-types": "~15.8.1",
    "react": "~18.3.1",
    "react-dom": "~18.3.1",
    "react-feather": "~2.0.10",
    "react-router-dom": "~5.3.4",
    "regenerator-runtime": "~0.14.1",
    "uuid": "~12.0.0"
  },
  "devDependencies": {
    "@babel/cli": "~7.28.3",
    "@babel/core": "~7.28.4",
    "@babel/preset-env": "~7.28.3",
    "@babel/preset-react": "~7.27.1",
    "@testing-library/dom": "~10.4.1",
    "@testing-library/react": "~16.3.0",
    "babel-jest": "~30.1.2",
    "babel-loader": "~10.0.0",
    "babel-plugin-prismjs": "~2.1.0",
    "cross-env": "~10.0.0",
    "css-loader": "~7.1.2",
    "dotenv": "~17.2.2",
    "eslint": "~8.57.1",
    "eslint-config-airbnb": "~19.0.4",
    "eslint-plugin-jsx-a11y": "~6.10.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-webpack-plugin": "~5.0.2",
    "file-loader": "~6.2.0",
    "html-webpack-plugin": "~5.6.4",
    "identity-obj-proxy": "~3.0.0",
    "jest": "~30.1.3",
    "path": "~0.12.7",
    "sass": "~1.91.0",
    "sass-loader": "~16.0.5",
    "style-loader": "~4.0.0",
    "stylelint": "~16.23.1",
    "stylelint-webpack-plugin": "~5.0.1",
    "svg-url-loader": "~8.0.0",
    "terser-webpack-plugin": "~5.3.14",
    "url-loader": "~4.1.1",
    "webpack": "~5.101.3",
    "webpack-cli": "~6.0.1",
    "webpack-dev-server": "~5.2.2",
    "webpack-merge": "~6.0.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
